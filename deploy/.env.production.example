# InCube Production Environment
# Copy to .env.production and fill in real values

# PostgreSQL
POSTGRES_PASSWORD=CHANGE_ME_random_password

# Auth — generate with: openssl rand -hex 32
JWT_SECRET=CHANGE_ME_random_256bit_secret

# AI — required for agent functionality
ANTHROPIC_API_KEY=sk-ant-REPLACE_ME
OPENAI_API_KEY=sk-REPLACE_ME
DEFAULT_AGENT_MODEL=claude-haiku-4-5-20251001

# Email (optional — needed for email features)
RESEND_API_KEY=re_REPLACE_ME
RESEND_FROM_EMAIL=noreply@incube.ai

# MinIO — dedicated InCube instance, generate with: openssl rand -hex 8 / openssl rand -hex 16
MINIO_ACCESS_KEY=CHANGE_ME_random_access_key
MINIO_SECRET_KEY=CHANGE_ME_random_secret_key

# Logging
LOG_LEVEL=INFO
